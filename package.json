{
  "name": "ocamlBetterErrors",
  "version": "0.1.0",
  "description": "Better errors for ocaml",
  "dependencies": {
    "@opam-alpha/re": "*",
    "@opam-alpha/reason": "1.13.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenglou/BetterErrors.git"
  },
  "author": "chenglou",
  "devDependencies": {
    "ocaml": "esy-ocaml/ocaml#esy"
  },
  "scripts": {
    "test": "ocaml tests/test.ml"
  },
  "esy": {
    "buildsInSource": true,
    "build": [
      "rebuild -pkg re.pcre -pkg unix src/berror.native",
      "cp _build/src/berror.native $cur__bin/berror"
    ]
  }
}
