{
  "name": "refmterr",
  "version": "3.1.7",
  "description": "Reason Formatting of Errors",
  "dependencies": {
    "@opam/dune": "*",
    "@opam/re": "*",
    "@esy-ocaml/reason": "^3.0.0",
    "ocaml": ">= 4.2.0  <= 4.7.0",
    "@reason-native/chalk": "link:../chalk"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esy-ocaml/BetterErrors.git"
  },
  "author": {
    "name": "chenglou"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "ocaml": "~4.6.0",
    "@esy-ocaml/reason": "3.3.3"
  },
  "buildDirs": {
    "lib": {
      "name": "refmterr.lib",
      "namespace": "Refmterr",
      "require": ["re.pcre", "unix", "chalk.lib"]
    },
    "bin": {
      "name": "berror.exe",
      "require": ["refmterr.lib", "unix", "re.pcre"],
      "main": "berror"
    }
  },
  "esy": {
    "build": [
      [
        "make",
        "build"
      ]
    ],
    "install": [
      [
        "make",
        "install"
      ]
    ]
  }
}
